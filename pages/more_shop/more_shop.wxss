}

.type-text .is_checked {
	color: #437AEA;
	border: 1px solid #437AEA;
	background-color: #fff;
  }

  .type-text .is_checked {
    position: relative;
    color: #437AEA;
    background-color: #fff;
    border-radius:7px;
    border:1px solid #437AEA;
}
.type-text .is_checked::before {
    content: '';
    position: absolute;
    right: 0;
    bottom: 0;
    border: 12px solid #437AEA;
    border-top-color: transparent;
    border-left-color: transparent;
    border-radius: 0 0 5px 0;
}
.type-text .is_checked::after {
    content: '';
    width: 2px;
    height: 6px;
    position: absolute;
    right: 5px;
    bottom: 3px;
    border: 1px solid #fff;
    border-top-color: transparent;
    border-left-color: transparent;
    transform: rotate(45deg);
}
