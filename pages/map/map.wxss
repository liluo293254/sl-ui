/* 底部详情框 start */
.shop-wrapper{
  position: absolute;
  z-index: 999;
  left: 0;
  right: 0;
  bottom: 40rpx;
  width: 92%;
  margin: 0 auto;
  padding: 20rpx 20rpx 0;
  overflow: hidden;
  background: #fff;
  border-radius: 10px;
  align-items: center;
  box-shadow: 2px 2px 10px 2px #eee;
}
.shop-wrapper .shop-img{
  width: 120px; 
  height: 120px; 
  border-radius: 6px; 
  overflow: hidden;
  float: left;
}
.shop-wrapper .shop-detail{
  flex: 1;
  padding: 0 12rpx;
  overflow: hidden;
  
}
.shop-wrapper .shop-detail .detail-icon1{
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx; 
}
.shop-wrapper .shop-detail .detail-icon2{
  width: 28rpx;
  height: 32rpx;
  margin-right: 10rpx; 
  float: left;
}
.shop-wrapper .shop-detail .detail-text{
  flex: 1;
  font-size: 12px;
  color: #727272;
}
.shop-wrapper .shop-detail .detail-type{
  color: #ee782e;
  font-size: 14px;
  margin: 4rpx 6rpx 4rpx 0;
  padding: 0 8rpx;
  border: thin solid #f9ae9b;
  border-radius: 4px;
}
.shop-wrapper .shop-navigation{
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
  /* background: url('data:image/png;base64,') no-repeat;  */
  /* background-size: contain; */
}
/* 底部详情框 end */

/* 隐藏菜单选择器 start */
.select-wrapper{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9;
  text-align: center;
  background-color: rgba(0, 0, 0, 0.4);
}
.select-wrapper:after{
  content: "";
  display: inline-block;
  width: 0;
  height: 100%;
  vertical-align: middle;
}
.select-wrapper .select-group{
  width: 30%;
  margin: 0 auto;
  vertical-align: middle;
  background-color: #fff;
}
.select-wrapper .select-group .select-field{
  padding: 20rpx 0;
}
.select-wrapper .select-group .select-field + .select-field{
  border: thin solid #eee;
}
.select-wrapper .select-group .select-field:active{
  color: #fff;
  background-color: #249dee;
}
/* 隐藏菜单选择器 end */
/* 新增卡片样式 */
.room {
