  /* padding: 0 10px; */
}

.toggle-header-bg {
  color: #fff;
  font-size: 14px;
  width: 100%;
  margin: 0 auto;
  overflow: hidden;
  height: 360rpx;
  position: relative;
  z-index: 0;

}

.toggle-left-flew {
  position: absolute;
  color: #fff;
  top: 10px;
  left: 10px;
  height: 30px;
  z-index: 1;
  padding: 0 15px !important;
  background: rgb(0 0 0 / 58%);
  line-height: 30px;
  font-size: 1px;
  border-radius: 30px;
}

.toggle-bottom-flew {
  position: absolute;
  color: #fff;
  bottom: 0px;
  left: 0;
  height: 40px;
  width: 100%;
  z-index: 1;
  padding: 5px 15px 10px !important;
  background: rgb(0 0 0 / 58%);
  line-height: 30px;
  font-size: 12px;
  border-radius: 0 0 10px 10px;
}

.toggle-bottom-title {
  font-size: 16px;
}

.toggle-bottom-info {
  font-size: 12px;
  float: right;
  letter-spacing: 3px;
}

.slide-swiper {
  height: 240px;
}

.image-wrapper {
  width: 100%;
  height: 0;
  /* padding-bottom: 75%; */
