@import '../../utils/iconfont.wxss';
.custom-navbar{
  position: fixed;
  display: flex;
  left: 0;
  right: 0;
  z-index: 1;
  align-items: center;
  background-color: #249dee;
}
.custom-navbar .custom-navback{
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: contain;
}
.custom-navbar .custom-navtitle{
  flex: 1;
  color: #fff;
  font-size: 16px;
  padding: 0 20rpx;
}
.custom-navbar .custom-navicon{
  width: 48rpx;
  height: 48rpx;
  /* padding: 8rpx; */
  margin-right: 20rpx;
  border-radius: 50px;
  background-color: #fff;
  box-shadow: 0 2px 12px 2px rgba(69,116,145, 0.25);
  line-height: 48rpx;
  font-size: 18rpx;
  text-align: center;
  color: red;
}
.custom-navbar .custom-navicon .icon{
  width: 32rpx;
  height: 32rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
}
.inline-block{
  display: inline-block;
}
.text-hidden{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.header{
  position: absolute;
  display: flex;
  z-index: 1;
  left: 0;
  right: 0;
  top: 65px;
  /* top: 40rpx; */
  /* top: 172rpx; */
  width: 100%;
  /* width: 92%; */
  margin: 0 auto;
}
.header .header-search{
  position: relative;
  display: flex;
  flex: 1;
  align-items: center;
  overflow: hidden;
  background-color: #fff;
  /* border-radius: 5px; */
  /* box-shadow: 0 2px 12px 2px rgba(69,116,145, 0.25); */
}
.header .header-search .search-type{
  position: relative;
  font-size: 12px;
  color: #bababa;
  padding: 10rpx 60rpx 10rpx 16rpx;
  overflow: hidden;
}
.header .header-search .search-type:before{
  position: absolute;
  content: '';
  z-index: 1;
  right: 0;
  top: 15%;
  bottom: 0;
  width: 1px;
  height: 70%;
  pointer-events: none;
  /* background: #dedede; */
}
 

.header .header-search .search-type image{
  position: absolute;
  content: '';
  z-index: 1;
  top: 30%;
  right: 20rpx;   
  width: 20rpx;
  height: 20rpx;
  pointer-events: none; 
}

.header .header-search .search-input{
  position: relative;
  flex: 1;
  border-left:1px solid #dedede;
  border-right:1px solid #dedede;
}
.header .header-search .search-input:before{
  position: absolute;
  content: '';
  z-index: 1;
  top: 12px;
  left: 20rpx;
  width: 40rpx;
  height: 40rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: contain;
}

