/* scroll item*/
.scroll-wrapper {
  white-space: nowrap;
  -webkit-overflow-scrolling: touch;
  background: #FFF;
  height: 90rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.navigate-item {
  display: inline-block;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0 20rpx;
}

.names {
  font-size: 28rpx;
  color: #3c3c3c;
}

.names.active {
  color: var(--color--);
  font-weight: bold;
  font-size: 30rpx;
}

.currtline {
  margin: -15rpx auto 0 auto;
  width: 40%;
  height: 8rpx;
  border-radius: 4rpx;
}

.currtline.active {
  background: var(--color--);
  transition: all .3s;
}

.swiper-item {
  width: 100%;
  text-align: center;
  padding: 32rpx 0;
}

scroll-view {
  height: auto;
